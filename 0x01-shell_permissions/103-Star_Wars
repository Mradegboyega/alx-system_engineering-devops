#!/bin/bash

cat << "EOF"
    ____ _   _ ____  ____       _
   / ___| | | |  _ \/ ___|  ___| |_ ___  _ __
  | |   | |_| | |_) \___ \ / _ \ __/ _ \| '__|
  | |___|  _  |  __/ ___) |  __/ || (_) | |
   \____|_| |_|_|   |____/ \___|\__\___/|_|
EOF

sleep 1

cat << "EOF"
  _____ _     ____  ____    _    _   _ _____ ____
 |_   _| |   |  _ \|  _ \  / \  | \ | | ____|  _ \
   | | | |   | |_) | | | |/ _ \ |  \| |  _| | | | |
   | | | |___|  __/| |_| / ___ \| |\  | |___| |_| |
   |_| |_____|_|    |____/_/   \_\_| \_|_____|____/
EOF

# Continue adding more ASCII art lines to represent the opening crawl...

sleep 1

